{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/tiny-tails-place-be/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/tiny-tails-place-be/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/rest-api",
  "projects": {
    "rest-api": {
      "type": "application",
      "root": "apps/rest-api",
      "entryFile": "main",
      "sourceRoot": "apps/rest-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/rest-api/tsconfig.app.json"
      }
    },
    "websoket": {
      "type": "application",
      "root": "apps/websoket",
      "entryFile": "main",
      "sourceRoot": "apps/websoket/src",
      "compilerOptions": {
        "tsConfigPath": "apps/websoket/tsconfig.app.json"
      }
    },
    "batch": {
      "type": "application",
      "root": "apps/batch",
      "entryFile": "main",
      "sourceRoot": "apps/batch/src",
      "compilerOptions": {
        "tsConfigPath": "apps/batch/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  }
}
